# Environment Configuration Example
# Copy this file to .env and update with your actual values

# API Configuration
VITE_API_BASE_URL=https://your-backend-api.com/api
VITE_API_VERSION=v1

# Authentication
VITE_JWT_SECRET=your-super-secret-jwt-key-here
VITE_SESSION_TIMEOUT=1800000

# Security
VITE_ENABLE_RATE_LIMITING=true
VITE_MAX_LOGIN_ATTEMPTS=5
VITE_LOCKOUT_DURATION=900000

# Admin Credentials (Backend should handle this)
# DO NOT store actual credentials here in production
VITE_ADMIN_EMAIL=admin@titweng.com

# Features
VITE_ENABLE_2FA=true
VITE_ENABLE_AUDIT_LOG=true
VITE_ENABLE_IP_WHITELIST=false

# Monitoring
VITE_ENABLE_ANALYTICS=false
VITE_LOG_LEVEL=error